echo $ac_n "checking for IMAP support""... $ac_c" 1>&6
echo "configure:4686: checking for IMAP support" >&5
# Check whether --with-imap or --without-imap was given.
if test "${with_imap+set}" = set; then
  withval="$with_imap"
  
    if test "$withval" = "yes"; then
		if test -f /usr/local/include/mail.h; then
			withval=/usr/local
		elif test -f /usr/include/mail.h; then
			withval=/usr
		elif test -f /usr/include/imap/mail.h; then
			withval=/usr
		fi
    fi
	if test "$withval" != "no"; then
		IMAP_DIR=$withval
		if test -f $IMAP_DIR/include/imap/mail.h; then
			IMAP_INC_DIR=$IMAP_DIR/include/imap
		else
			IMAP_INC_DIR=$withval/include
		fi
		if test ! -f $IMAP_INC_DIR/mail.h; then
			{ echo "configure: error: could not find mail.h in $IMAP_INC_DIR !" 1>&2; exit 1; }
		fi
		if test ! -f  $IMAP_INC_DIR/rfc822.h; then
			{ echo "configure: error: could not find rfc822.h in $IMAP_INC_DIR !" 1>&2; exit 1; }
		fi
		if test ! -f $IMAP_INC_DIR/linkage.h; then
			{ echo "configure: error: could not find linkage.h in $IMAP_INC_DIR !" 1>&2; exit 1; }
		fi
		if test -n "$APXS"; then
			if test -f $IMAP_DIR/lib/libimap.a; then
				IMAP_LIBS="${apxs_runpath_switch}$IMAP_DIR/lib' -L$IMAP_DIR/lib -limap"
#           for FreeBSD
			elif test -f $IMAP_DIR/lib/libc-client4.a; then
				IMAP_LIBS="${apxs_runpath_switch}$IMAP_DIR/lib' -L$IMAP_DIR/lib -lc-client4"
			elif test ! -f $IMAP_DIR/lib/libc-client.a; then
				if test ! -f $IMAP_DIR/lib/libc-client4.so; then
					{ echo "configure: error: You need to copy or link $IMAP_DIR/lib/c-client.a to $IMAP_DIR/lib/libc-client.a" 1>&2; exit 1; }
				else
					IMAP_LIBS="${apxs_runpath_switch}$IMAP_DIR/lib' -L$IMAP_DIR/lib -lc-client4"
				fi
			else
				IMAP_LIBS="${apxs_runpath_switch}$IMAP_DIR/lib' -L$IMAP_DIR/lib -lc-client"
			fi
		else
			if test -f $IMAP_DIR/lib/libimap.a; then
				IMAP_LIBS="-L$IMAP_DIR/lib -limap"
#           for FreeBSD
			elif test -f $IMAP_DIR/lib/libc-client4.a; then
				IMAP_LIBS="${apxs_runpath_switch}$IMAP_DIR/lib' -L$IMAP_DIR/lib -lc-client4"
			elif test ! -f $IMAP_DIR/lib/c-client.a; then
				if test ! -f $IMAP_DIR/lib/libc-client4.so; then
					{ echo "configure: error: could not find c-client.a in $IMAP_DIR/lib !" 1>&2; exit 1; }
				else
					IMAP_LIBS="${ld_runpath_switch}$IMAP_DIR/lib -L$IMAP_DIR/lib -lc-client4"
				fi
			else
				IMAP_LIBS="-L$IMAP_DIR/lib $IMAP_DIR/lib/c-client.a"
			fi
		fi
		IMAP_INCLUDE="-I$IMAP_INC_DIR/"
		
		
        cat >> confdefs.h <<\EOF
#define HAVE_IMAP 1
EOF

	    echo "$ac_t""yes" 1>&6
	else
	    echo "$ac_t""no" 1>&6
	fi

