    ORACLE_VERSION="`echo  | cut -c 2-4`"
    test -z "$ORACLE_VERSION" && ORACLE_VERSION=7.3
  else
    ORACLE_VERSION=7.3
  fi
  echo "$ac_t""$ORACLE_VERSION" 1>&6


    # Oracle libs - nightmare :-)

    ORACLE_LIBDIR=lib
    ORACLE_LFLAGS=-L$ORACLEINST_TOP/$ORACLE_LIBDIR
